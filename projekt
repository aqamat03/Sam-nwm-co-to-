#Projekt oprogramowania Firmy Kuriejskiej

print("FIRMA KURIERSKA AQAPACK")

zmienna1 = "Witaj"
zmienna2 = input("Podaj login: ")

print(zmienna1 + " " + zmienna2)

print("NADAJ PACZKĘ")

zmienna3 = "Nadano paczkę! Dana paczki:"
zmienna4 = input("Podaj adres: ")
zmienna5 = input("Podaj miejscowość: ")
zmienna6 = input("Podaj kod pocztowy: ")
zmienna7 = input("Podaj kod kuriera: ")

print(zmienna3 + " Adres: " + zmienna4 + " Miejscowość: " + zmienna5 + " Kod pocztowy: " +zmienna6 + " Kod kuriera " + zmienna7)
